{
  "name": "api",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/app.js",
  "scripts": {
    "postinstall": "prisma generate",
    "build": "rimraf dist && pnpm prisma:generate && tspc",
    "dev": "pnpm prisma:migrate:deploy:dev && pnpm prisma:generate:dev && dotenv -e .env.local -- tsx watch src/app.ts",
    "start": "node dist/app.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:deploy:dev": "dotenv -e .env.local -- prisma migrate deploy",
    "prisma:generate:dev": "dotenv -e .env.local -- prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.local -- prisma migrate dev",
    "prisma:push:dev": "dotenv -e .env.local -- prisma db push",
    "prisma:reset:dev": "dotenv -e .env.local -- prisma migrate reset",
    "prisma:db:seed:dev": "dotenv -e .env.local -- prisma db seed",
    "prisma:migrate:deploy:staging": "dotenv -e .env.staging -- prisma migrate deploy",
    "prisma:generate:staging": "dotenv -e .env.staging -- prisma generate",
    "prisma:db:seed:staging": "dotenv -e .env.staging -- prisma db seed",
    "studio": "dotenv -e .env.local -- prisma studio",
    "lint": "tsc --noEmit && eslint .",
    "format": "prettier --write ."
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "eslint": "^9.6.0",
    "globals": "^15.8.0",
    "prisma": "^6.1.0",
    "rimraf": "5.0.5",
    "ts-patch": "3.1.2",
    "tsx": "4.11.0",
    "typescript": "5.4.4",
    "typescript-eslint": "^7.15.0",
    "typescript-transform-paths": "3.4.7"
  },
  "dependencies": {
    "@prisma/client": "^6.1.0",
    "cors": "2.8.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "http-status-codes": "^2.3.0",
    "morgan": "1.10.0",
    "serverless-http": "3.2.0"
  }
}
